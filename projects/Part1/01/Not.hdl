/**
 * Not gate:
 * out = not in
 * 
 * |  in   |  out  |
 * |   0   |   1   |
 * |   1   |   0   |
 *
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
	
	//Use Nand since:
	//   Not(in AND in)
	// = Not(in)
	
    Nand (a=in, b=in, out=out);
}