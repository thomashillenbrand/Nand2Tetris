/**
 * Zero gate: 
 *  out = 0
 *
 * |   in  |  out  |
 * |   0   |   0   |
 * |   0   |   1   |
 * |   1   |   0   |
 * |   1   |   1   |
 *
 */

CHIP Zero {
    IN in;
    OUT out;

    PARTS:

    Not(in=in, out=notin);
    And(a=in, b=notin, out=out);
    
}
